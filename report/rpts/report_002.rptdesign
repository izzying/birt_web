            <xml-property name="queryText"><![CDATA[select a.`order_date`,a.`total_count` ,b.`send_out_count` from 
(SELECT left(created_at,10) as `order_date`, count(distinct(device_id)) as `total_count` FROM 
orders where created_at>=?
and created_at<=?
and deleted_at is null
and client_id is null
and come_from='Union_App'
group by `order_date` 
) as a

left join
(
SELECT left(created_at,10) as `order_date`, count(distinct(device_id)) as `send_out_count` FROM 
orders where created_at>=?
and created_at<=?
and deleted_at is null
and client_id is null
and come_from='Union_App'
and export_to_erp=true
group by `order_date` 
) as b

on a.`order_date`=b.`order_date`]]></xml-property>

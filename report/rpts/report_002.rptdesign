            <xml-property name="queryText"><![CDATA[select a.`date`,a.`count` as `total`,b.`count` as `send_out` from 
(SELECT left(created_at,10) as `date`, count(distinct(device_id)) as `count` FROM 
orders where created_at>=?
and deleted_at is null
and client_id is null
and come_from='Union_App'
group by `date` 
) as a

left join
(
SELECT left(created_at,10) as `date`, count(distinct(device_id)) as `count` FROM 
orders where created_at>=?
and deleted_at is null
and client_id is null
and come_from='Union_App'
and export_to_erp=true
group by `date` 
) as b

on a.`date`=b.`date`]]></xml-property>

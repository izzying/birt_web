            <xml-property name="queryText"><![CDATA[select orders.`coordinator_id`,
       users.`username`,
       count(DISTINCT(orders.device_id+ date(`orders` .assigned_at))) as `assign_count`,
       sum(orders.`state`in('已发货，准备收货', '等待配货', '正在配货', '客户签收，订单完成', '客户拒签,原件返回')) as `send_out_count`,
       sum(orders.`export_to_erp`) as `send_out_shipping_count`,
       sum(shippingorders.`state` in(6, 8)) as `sign_ok_count`,
       sum(shippingorders.`state` in(7, 9)) as `sign_fail_count`,
       sum(shippingorders.`state` in(6, 8, 7, 9)) as `complete_count`
  from orders
  INNER JOIN `devices` on `devices` .device_id= orders.`device_id`
  inner join users on orders.`coordinator_id`= users.id
  left join `shippingorders` on `shippingorders` .id= `orders` .shippingorder_id
 where orders.assigned_at> ?
   and orders.assigned_at< date_add(date(?), INTERVAL 1 DAY)
   and(`come_from`= 'Self_App'
    or `client_id`> 0)
 group by `coordinator_id`]]></xml-property>

            <xml-property name="queryText"><![CDATA[select orders.`coordinator_id`,
       users.`username`,
       count(DISTINCT(orders.device_id)) as `assign_count`,
       count(DISTINCT(`devices` .id*orders.`export_to_erp`)) -1 as `send_out_count`
  from orders
  INNER JOIN `devices` on `devices` .device_id= orders.`device_id`
  inner join users on orders.`coordinator_id`= users.id
 where orders.assigned_at> ?
   and orders.assigned_at< date_add(date(?), INTERVAL 1 DAY)
   and(`come_from`= 'Self_App'
    or `client_id`> 0)
 group by `coordinator_id`]]></xml-property>

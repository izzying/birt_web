            <xml-property name="queryText"><![CDATA[select `adv_contents` .id,
       `adv_contents`.`title`,
       `adv_contents` .price,
       a.*,
       b.`count` as active_count
  from `adv_contents`
  inner join(
select `adv_content_id`, SUM(`read_count`) as r_count, `install_count` as i_count
  from `adv_statistics`
 where date(`created_at`)= '2016-01-27'
 GROUP BY `adv_content_id`) as a on a.adv_content_id= `adv_contents` .id
  left join `adv_advertiser_reports` as b on b.`adv_content_id`= a.adv_content_id
 where `adv_contents`.`activity`= true
   and `adv_contents`.`trash`= false
 order by i_count desc]]></xml-property>
